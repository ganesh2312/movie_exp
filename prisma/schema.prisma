// prisma/schema.prisma
generator client {
  provider = "prisma-client-js"
}

// Add this datasource block â–¼
datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  email     String   @unique
  password  String
  favorites String[]
  createdAt DateTime @default(now())
}